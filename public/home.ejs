                <span>Dashboard</span>
            </div>

            <div class="DBcontent"></div>
            <div class="DBcontent"></div>
            <div class="DBcontent"></div>
        </div>
        <div class="analytics" id="Gross">
            <div class="nav" id="navBar">
                <%- include("./partials/nav") %>
            </div>
            <div class="container-main" id="container-main">

            </div>
        </div>
    </div>
</body>
</html>